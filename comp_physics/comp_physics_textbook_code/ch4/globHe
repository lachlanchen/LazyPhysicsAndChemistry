      IMPLICIT NONE

      INTEGER N, NSize

      PARAMETER (N=4, NSize=4)

      DOUBLE PRECISION Z, PI, Alpha(NSize), C(NSize), 
     .       DensMat(NSize, NSize),
     .       SMatrix(NSize,NSize), 
     .       Hamilton(NSize,NSize), 
     .       VMatrix(NSize,NSize), GMatrix(NSize,NSize),
     .       FMatrix(NSize,NSize), 
     .       QMatrix(NSize,NSize,NSize,NSize)

      INTEGER TypeMatrix(NSize,NSize,NSize,NSize)

      COMMON Z, Alpha, C, SMatrix, Hamilton, VMatrix, 
     .       QMatrix, DensMat, FMatrix, TypeMatrix, GMatrix, 
     .       PI
  


     
